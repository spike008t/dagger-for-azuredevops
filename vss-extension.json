{
    "manifestVersion": 1,
    "id": "dagger-for-azuredevops",
    "name": "Dagger for Azure Devops",
    "version": "0.1.7",
    "publisher": "TieuPhilippeKhim",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "tags": [
        "dagger"
    ],
    "description": "Tools for building/releasing with Dagger.",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/dagger-128x128.png"
    },
    "files": [
        {
            "path": "images",
            "addressable": true
        },
        {
            "path": "dist"
        }
    ],
    "contributions": [
        {
            "id": "dagger-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "dist"
            }
        }
    ]
}